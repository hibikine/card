  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "setupFiles": ["<rootDir>/setup-file.js"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn tslint",
      "git add"
    ],
    "*.ts": [
      "yarn tslint",
      "git add"
    ],
    "*.tsx": [
      "yarn tslint",
      "git add"
    ]
  }
